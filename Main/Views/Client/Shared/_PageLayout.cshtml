@using MSLivingChoices.Mvc.Uipc.Client.Helpers
@model MSLivingChoices.Mvc.Uipc.Client.ViewModels.SearchVm

@{
    Layout = "~/Views/Client/Shared/_BaseLayout.cshtml";
}

@section scripts
{
    <script type="text/javascript">
        mslc.resolve(['client/widgets/carousel'], function (carousel) {
            carousel.init('featured-services-carousel');
        }, 'init-carousel');
    </script>

    @if (IsSectionDefined("scripts"))
    {
        @RenderSection("Scripts")
    }
}

@RenderBody()
