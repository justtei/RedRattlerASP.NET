            height: 120px;
        }

        .select {
            position: relative;
            background-color: #fff;
            width: 100%;
        }

        .input-group > * {
            display: table-cell;
            vertical-align: middle;
        }

        .select .caption {
            padding-right: 30px;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
            cursor: pointer;
        }
        #ui-datepicker-div {
            z-index: 99999 !important
        }
        .ui-datepicker {
            width: 250px; /*what ever width you want*/
        }
        .ui-datepicker .ui-datepicker-header {
            background: dodgerblue;
        }
        .ui-datepicker .ui-datepicker-header .ui-datepicker-next .ui-icon {
            width: 100%;
            height: 100%;
        }
        .ui-datepicker .ui-datepicker-header .ui-datepicker-prev .ui-icon {
            width: 100%;
            height: 100%;
        }
        .ui-datepicker .ui-datepicker-prev, .ui-datepicker .ui-datepicker-next {
            top: 8px;
            width: 1em;
            height: 1em;
        }
        [class*='list-col-'].one-line {
            white-space: nowrap;
            -webkit-column-gap: 20px;
            -moz-column-gap: 20px;
            column-gap: 20px;
        }

        [class*='list-col-'] {
            list-style: none;
            min-height: initial;
        }
        .ind-s {
            margin-top: 10px;
        }

        address, .address {
            margin-bottom: 0;
            font-style: normal;
            color: #000;
            font-weight: bold;
        }
    </style>
