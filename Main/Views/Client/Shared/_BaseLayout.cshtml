    <script type="text/javascript">
            mslc.resolve(['lib/jQuery', 'lib/ko', 'client/widgets/searchBar'], function($, ko, SearchBar) {
                var model = new SearchBar(@Html.Json(Model.SearchBar), $('[data-search="form"]'));
                var searchBar = document.getElementById('search-bar');
                ko.applyBindings(model, searchBar);
            }, 'search-bar-init');

            mslc.resolve(['client/analytics/google', 'lib/jQuery'], function(ga, $) {
                $('footer a[data-link-type]').on('click', function() {
                    var linkType = $(this).data('link-type');

                    ga.trackLinkVisit('Footer', linkType);
                });

                $('[data-ga="book-order"]').on('click', function() {
                    ga.trackLinkVisit('Book Order');
                });
            }, 'ga-out-links-tracking');
    </script>
    <script>
