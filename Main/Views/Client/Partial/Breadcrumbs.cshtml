@model List<MSLivingChoices.Mvc.Uipc.Client.ViewModels.LinkVm>

@if (Model.Any())
{
    <ol class="breadcrumb container">
        @* Render all breadcrumbs as link except the last *@
        @foreach (var crumb in Model.Take(Model.Count - 1))
        {
        <li itemscope itemtype="http://data-vocabulary.org/Breadcrumb">
            <a href="@crumb.Href" itemprop="url">
                <span itemprop="title">
                    @crumb.InnerText
                </span>
            </a>
            <span style="margin:0 3px"><</span>
        </li>
            
        }
        <li>
            <span>@Model.Last().InnerText</span>
        </li>
    </ol>
}
