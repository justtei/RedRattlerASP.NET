@using MSLivingChoices.Configuration
@using MSLivingChoices.Localization
@using MSLivingChoices.Mvc.Uipc.Client.Helpers
@{
    var config = ConfigurationManager.Instance;
}
<script type="text/javascript">
    mslc.resolve(['client/config', 'client/text'], function(config, text) {
        config.setting('dateFormat', '@config.ClientJsDateFormat');
        config.setting('momentJsTimeFormat', '@config.MomentJsTimeFormat');
        config.setting('slideDuration', @config.SlideDuration);
        config.setting('timeFormat', '@config.TimeFormat');
        config.setting('googleAnalyticsKey', '@config.GoogleAnalyticsKey');
        config.setting('carouselShiftDelay', '@config.CarouselShiftDelay');
        config.setting('autocompleteDelay', @config.AutocompleteTimerDelay);
        // TODO: move to local content and replace lines color with brand.
        config.setting('statesKmzUrl', 'http://www.realestatebook.com/Content/Kml/us_states.kmz');

        text.message('ajaxServerErrorMessage', '@HttpUtility.JavaScriptStringEncode(ErrorMessages.AjaxServerError)');
        text.message('noResutlsErrorMessage', '@HttpUtility.JavaScriptStringEncode(ErrorMessages.SearchBar_NoResults)');

        @foreach (var pair in MSLivingChoices.Localization.Validation.ResourceManager.GetLocalization())
        {
            @:text.message('@pair.Key', '@HttpUtility.JavaScriptStringEncode(pair.Value)');
        }
    });
</script>